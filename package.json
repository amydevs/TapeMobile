  "main": "app/background.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production cross-env NODE_PWA=true webpack",
    "watch": "cross-env NODE_ENV=development webpack",
    "serve": "cross-env NODE_ENV=development webpack serve",
    "serve:prod": "cross-env NODE_ENV=production cross-env NODE_PWA=true webpack serve",
    "sync": "cap sync",
    "extract": "node ./input/extract.js",
    "standalone:serve": "node standalone",
    "standalone:build": "caxa --identifier \"tape-mobile-installer-files\" -i \"./\" -o \"standalone/dist/tapemobileinstaller\" --exclude \"./node_modules/.bin\" \"app/*\" \"!app/polyfills\" \"dist/*\" \"!dist/manifest.json\" \"input/app.*\" \"standalone/dist/*\" \"android/*.apk\" \"android/*.aar\" \"android/*.ap_\" \"android/*.aab\" \"android/*.dex\" \"android/*.class\" \"android/bin/\" \"android/gen/\" \"android/out/\" \"android/.gradle/\" \"android/build/\" \"android/local.properties\" \"android/proguard/\" \"android/*.log\" \"android/.navigation/\" \"android/captures/\" \"android/*.iml\" \"android/.idea/workspace.xml\" \"android/.idea/tasks.xml\" \"android/.idea/gradle.xml\" \"android/.idea/assetWizardSettings.xml\" \"android/.idea/dictionaries\" \"android/.idea/libraries\" \"android/.idea/caches\" \"android/.idea/modules.xml\" \"android/.idea/navEditor.xml\" \"android/.externalNativeBuild\" \"android/.cxx/\" \"android/freeline.py\" \"android/freeline/\" \"android/freeline_project_description.json\" \"android/fastlane/report.xml\" \"android/fastlane/Preview.html\" \"android/fastlane/screenshots\" \"android/fastlane/test_output\" \"android/fastlane/readme.md\" \"android/vcs.xml\" \"android/lint/intermediates/\" \"android/lint/generated/\" \"android/lint/outputs/\" \"android/lint/tmp/\" \"android/*.hprof\" \"android/capacitor-cordova-android-plugins\" \"android/app/src/main/assets/public\" --no-dedupe -- \"{{caxa}}/node_modules/.bin/node\" \"{{caxa}}/standalone/index.js\"",
    "standalone:gitignore": "node ./standalone/gitignore.js"
  },
  "dependencies": {
    "@capacitor/android": "^3.4.3",
    "@capacitor/app": "^1.1.1",
    "@capacitor/core": "^3.4.3",
    "@capacitor/filesystem": "^1.1.0",
    "assert": "^2.0.0",
    "browserfs": "^1.4.3",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "detect-csv": "^1.1.0",
    "fs-jetpack": "^0.10.2",
    "jquery": "^3.2.1",
    "json-storage": "^2.1.1",
    "junk": "^2.1.0",
    "localStorage": "^1.0.3",
    "lodash": "^4.17.4",
    "mobile-drag-drop": "^2.3.0-rc.2",
    "natives": "^1.1.6",
    "run-sequence": "^1.2.2",
    "stream-browserify": "^3.0.0",
    "typescript": "^4.6.2",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "@capacitor/cli": "^3.4.3",
    "@types/jquery": "^3.5.14",
    "@types/jsdom": "^16.2.14",
    "asar": "^3.1.0",
    "caxa": "^2.1.0",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "fs-extra": "^10.0.1",
    "html-webpack-plugin": "^5.5.0",
    "jsdom": "^19.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.8",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "workbox-webpack-plugin": "^6.5.1"
  },
  "pkg": {
    "scripts": "standalone/**/*.js",
    "options": [
      "experimental-modules"
    ]
  }
